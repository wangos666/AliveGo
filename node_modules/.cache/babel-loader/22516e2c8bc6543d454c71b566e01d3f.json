st\">\r\n            <ul class=\"yui3-g\">\r\n              <li class=\"yui3-u-1-5\" v-for=\"item in goodsList\">\r\n                <div class=\"list-wrap\">\r\n                  <div class=\"p-img\">\r\n                    <a href=\"item.html\" target=\"_blank\"><img :src=\"item.defaultImg\" /></a>\r\n                  </div>\r\n                  <div class=\"price\">\r\n                    <strong>\r\n                      <em>¥</em>&nbsp;\r\n                      <i> {{ item.price }}</i>\r\n                    </strong>\r\n                  </div>\r\n                  <div class=\"attr\">\r\n                    <a target=\"_blank\" href=\"item.html\" :title=\"item.title\">{{ item.title }}</a>\r\n                  </div>\r\n                  <div class=\"commit\">\r\n                    <i class=\"command\">已有<span>2000</span>人评价</i>\r\n                  </div>\r\n                  <div class=\"operate\">\r\n                    <a href=\"success-cart.html\" target=\"_blank\" class=\"sui-btn btn-bordered btn-danger\">加入购物车</a>\r\n                    <a href=\"javascript:void(0);\" class=\"sui-btn btn-bordered\">收藏</a>\r\n                  </div>\r\n                </div>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <div class=\"fr page\">\r\n            <div class=\"sui-pagination clearfix\">\r\n              <ul>\r\n                <li class=\"prev disabled\">\r\n                  <a href=\"#\">«上一页</a>\r\n                </li>\r\n                <li class=\"active\">\r\n                  <a href=\"#\">1</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">2</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">3</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">4</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">5</a>\r\n                </li>\r\n                <li class=\"dotted\"><span>